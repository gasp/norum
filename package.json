{
  "name": "minimalist-express",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "sequelize": "sequelize",
    "initialize": "npm run sequelize db:migrate && npm run sequelize db:seed:all",
    "reset": "rm database.sqlite3 && npm run initialize",
    "lint": "prettier --write './**.js' './*/*.js'"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "pug": "^2.0.4",
    "sequelize": "^5.19.0",
    "stylus": "0.54.5"
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.0"
  }
}
